<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
  <display-name>Simple</display-name>
  
  <servlet>
    <servlet-name>CodeRunnerRootServlet</servlet-name>  
    <servlet-class>com.cloudmine.coderunnerwrapper.CodeRunnerRootServlet</servlet-class>  
  </servlet>
  <servlet>
    <servlet-name>CodeSnippetNameServlet</servlet-name>
    <servlet-class>com.cloudmine.coderunnerwrapper.CodeSnippetNameServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CodeRunnerRootServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CodeSnippetNameServlet</servlet-name>
    <url-pattern>/names</url-pattern>
  </servlet-mapping>
</web-app>
